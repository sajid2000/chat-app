{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "no-underscore-dangle": 0,
        "comma-dangle": 0,
        "import/prefer-default-export": 0,
        "import/extensions": [
            0,
            {
                "js": "always"
            }
        ],
        "no-console": 0,
        "no-var": "warn",
        "one-var": "warn",
        "indent": 0,
        "linebreak-style": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4,
                "semi": true,
                "endOfLine": "auto"
            }
        ]
    },
    "plugins": [
        "prettier"
    ]
}